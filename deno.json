{
  "name": "@raptor/router",
  "version": "0.20.0",
  "exports": "./mod.ts",
  "publish": {
    "exclude": [
      ".github",
      "examples",
      "docs",
      "**/*.test.ts"
    ]
  },
  "tasks": {
    "test": "APP_URL=http://localhost:8000 deno test --coverage=coverage-report --allow-all --parallel",
    "test-coverage": "APP_URL=http://localhost:8000 deno coverage coverage-report"
  },
  "fmt": {
    "exclude": [
      "README.md"
    ]
  },
  "compilerOptions": {
    "types": [
      "./types.d.ts"
    ]
  },
  "lock": true,
  "imports": {
    "@raptor/kernel": "jsr:@raptor/kernel@^0.19.0",
    "@std/assert": "jsr:@std/assert@^1.0.19"
  }
}
